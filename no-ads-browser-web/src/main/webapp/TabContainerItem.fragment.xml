<core:FragmentDefinition
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:l="sap.ui.layout"
	xmlns="sap.m">
	<TabContainerItem name="{/title}" modified="false" width="100%">
		<content>
			<l:HorizontalLayout width="100%">
			<!--FlexBox justifyContent="Start" width="100%"-->
				<Input value="{/url}" fieldWidth="70%"/>
				<Button text="{i18n>goButtonText}" press="go"/>
				<Button text="{i18n>optionsButtonText}" press="options"/>
			<!--/FlexBox-->
			</l:HorizontalLayout>
		</content>
	</TabContainerItem>
</core:FragmentDefinition>